{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "s3:GetBucketLocation",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor3",
      "Effect": "Allow",
      "Action": "s3:HeadBucket",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    },
    {
      "Sid": "ListBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::john-dev"
      ],
      "Condition": {
        "StringEquals": {
          "s3:prefix": [
            "",
            "developergroup1/",
            "developergroup1/inputs",
            "developergroup1/results",
            "logs",
            "developergroup1/submissions",
            "developergroup1/configs",
            "developergroup1/inputs/",
            "developergroup1/results/",
            "developergroup1/submissions/",
            "developergroup1/configs/"
          ],
          "s3:delimiter": [
            "/"
          ]
        }
      }
    },
    {
      "Sid": "ListSubBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::john-dev"
      ],
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "developergroup1/inputs/*",
            "developergroup1/results/*",
            "developergroup1/configs/*",
            "developergroup1/submissions/*"
          ]
        }
      }
    },
    {
      "Sid": "Inputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::john-dev/developergroup1/inputs/*",
        "arn:aws:s3:::john-dev/developergroup1/configs/*",
        "arn:aws:s3:::john-dev/developergroup1/submissions/*"
      ]
    },
    {
      "Sid": "Outputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::john-dev/developergroup1/results/*",
        "arn:aws:s3:::john-dev/developergroup1/configs/*"
      ]
    }
  ]
}